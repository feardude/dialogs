          <stringProp name="script">// Генерация случайных значений для from и to
def from = (1..1000).toList().get(new Random().nextInt(1000))
def to = (1..1000).toList().get(new Random().nextInt(1000))

// Проверка на совпадение
while (from == to) {
    to = (1..1000).toList().get(new Random().nextInt(1000))
}

vars.put(&quot;from&quot;, from.toString())
vars.put(&quot;to&quot;, to.toString())
</stringProp>
